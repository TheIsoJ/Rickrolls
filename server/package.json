{
  "name": "rickrolls-api",
  "version": "1.0.0",
  "description": "API for getting data from Rickrolls.",
  "main": "dist/server.js",
  "private": true,
  "type": "module",
  "scripts": {
    "build:dev": "tsc --watch",
    "build:prod": "tsc",
    "start:migrate:dev": "prisma migrate dev && npm run start:dev",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "start:dev": "nodemon dist/server.js",
    "start:prod": "npm run build:prod && node dist/server.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "author": "Jesse Keskel√§ <juiceneblueyt@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "stripe": "^11.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.10",
    "nodemon": "^2.0.20",
    "prisma": "^4.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
