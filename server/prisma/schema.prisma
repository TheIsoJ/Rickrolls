generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Rickroll {
  id                String  @id @default(uuid())
  name              String  @db.LongText
  description       String? @db.LongText
  link              String  @db.LongText
  videoId           String  @db.LongText
  rickroll_cta_link String  @db.LongText
  slug              String  @db.LongText
}

model User {
  id                     String  @id @default(uuid())
  name                   String  @db.LongText
  email                  String? @db.LongText
  api_key                String  @db.LongText
  username               String  @db.LongText
  profile_picture        String? @db.LongText
  password               String? @db.LongText
  stripe_secret_key      String? @db.LongText
  stripe_publishable_key String? @db.LongText
}
